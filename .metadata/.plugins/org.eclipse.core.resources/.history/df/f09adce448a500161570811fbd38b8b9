
public class LoopingPractice 
{

	public static void main(String[] args) 
	{
		
	
//		boolean running = true;
//		
//		while (running)
//		{
//			if (gameOver)
//				running = false;
//		}
		
		boolean kitten = true;
		
		while (kitten)
		{
			System.out.println("KITTENS");
		}
		
		int	TNT = 42;
		
		while (TNT < 1000000)
		{
			System.out.println("not Enough TNT!!!!; Current TNT: " + TNT);
			TNT++;
		}
	}

}
