
public class sleepOver 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub

		boolean workDone = true;
		
		boolean roomClean = true;
		
		if (roomClean && workDone)
		{
			System.out.print ("go to sleep over"); 
		}
		else
		{
			System.out.print ("don't go to sleep over");
			
		}
			
		
		
	}

}
