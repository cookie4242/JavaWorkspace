	{
	System.out.print("success");	
	}
	 if (weekend = false && cigars > 39 && cigars < 61)
	{
	System.out.print("success");	
	}
	 else	
	{
	System.out.print ("Awwww Nuts");	
	}
		
		
		
		
		
		
	}

